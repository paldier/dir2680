#***** hnap wan action for IPv6 auto detect ******
arca.dbus.firewall:FWD_FAIL
arca.dbus.misc:waniface CMD_BOOT
				wan CMD_WAN_START	
arca.dbus.dhcpc:CMD_RESTART
arca.dbus.wanppp:CMD_STOP
arca.dbus.autowandetect:CMD_AUTO_IP6_RESTART
arca.dbus.dhcpd:CMD_RESTART
arca.dbus.dnsmasq:CMD_RESTART
arca.dbus.upnp:CMD_RESTART
arca.dbus.misc:deviceready 1
