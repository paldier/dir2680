#!/bin/sh

#Account
abscfg set ARC_USB_ACCOUNT_TotalUserCount 2

# account 1
abscfg set ARC_USB_ACCOUNT_x_Name 0 user0001
abscfg set ARC_USB_ACCOUNT_x_Locked 0 1
abscfg set ARC_USB_ACCOUNT_x_Group 0 normal
abscfg set ARC_USB_ACCOUNT_x_Password 0 12345678
abscfg set ARC_USB_ACCOUNT_x_Privilege 0 1
abscfg set ARC_USB_ACCOUNT_x_AccountId 0 1000
abscfg set ARC_USB_ACCOUNT_x_LinuxUid 0 1001
abscfg set ARC_USB_ACCOUNT_x_HomeDiskSn 0 NA
abscfg set ARC_USB_ACCOUNT_x_HomeMountPoint 0 /tmp/media
abscfg set ARC_USB_ACCOUNT_x_HomePath 0 /
abscfg set ARC_USB_ACCOUNT_x_SAMBA_ShareFolderCount 0 1
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderDiskSn 0 0 NA
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderMountpoint 0 0 /tmp/media
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderPath 0 0 /
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderDisplayName 0 0 SAMBA1
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderReadOnly 0 0 1
abscfg set ARC_USB_ACCOUNT_x_FTP_ReadOnly 0 1
abscfg set ARC_USB_ACCOUNT_x_FTP_HomeDiskSn 0 NA
abscfg set ARC_USB_ACCOUNT_x_FTP_HomeMountpoint 0 /tmp/media
abscfg set ARC_USB_ACCOUNT_x_FTP_HomePath 0 /
abscfg set ARC_USB_ACCOUNT_x_FTP_FtpEnable 0 0
abscfg set ARC_USB_ACCOUNT_x_FTP_FtpWanAccess 0 1
abscfg set ARC_USB_ACCOUNT_x_FTP_FtpsEnable 0 0 
abscfg set ARC_USB_ACCOUNT_x_FTP_FtpsWanAccess 0 1

# account 2
abscfg set ARC_USB_ACCOUNT_x_Name 1 user0002
abscfg set ARC_USB_ACCOUNT_x_Locked 1 1
abscfg set ARC_USB_ACCOUNT_x_Group 1 normal
abscfg set ARC_USB_ACCOUNT_x_Password 1 12345678
abscfg set ARC_USB_ACCOUNT_x_Privilege 1 0
abscfg set ARC_USB_ACCOUNT_x_AccountId 1 1000
abscfg set ARC_USB_ACCOUNT_x_LinuxUid 1 1001
abscfg set ARC_USB_ACCOUNT_x_HomeDiskSn 1 NA
abscfg set ARC_USB_ACCOUNT_x_HomeMountPoint 1 /tmp/media
abscfg set ARC_USB_ACCOUNT_x_HomePath 1 /
abscfg set ARC_USB_ACCOUNT_x_SAMBA_ShareFolderCount 1 2
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderDiskSn 1 0 NA
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderDiskSn 1 1 NA
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderMountpoint 1 0 /tmp/media
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderMountpoint 1 1 /tmp
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderPath 1 0 /
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderPath 1 1 /
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderDisplayName 1 0 SAMBA1
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderDisplayName 1 1 SAMBA2
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderReadOnly 1 0 0
abscfg set ARC_USB_ACCOUNT_x_SAMBA_y_ShareFolderReadOnly 1 1 0
abscfg set ARC_USB_ACCOUNT_x_FTP_ReadOnly 1 0
abscfg set ARC_USB_ACCOUNT_x_FTP_HomeDiskSn 1 NA
abscfg set ARC_USB_ACCOUNT_x_FTP_HomeMountpoint 1 /tmp/media
abscfg set ARC_USB_ACCOUNT_x_FTP_HomePath 1 /
abscfg set ARC_USB_ACCOUNT_x_FTP_FtpEnable 1 0
abscfg set ARC_USB_ACCOUNT_x_FTP_FtpWanAccess 1 1
abscfg set ARC_USB_ACCOUNT_x_FTP_FtpsEnable 1 0 
abscfg set ARC_USB_ACCOUNT_x_FTP_FtpsWanAccess 0 1

#Samba
abscfg set ARC_USB_SMB_SRVName Test
abscfg set ARC_USB_SMB_Comment Comment
abscfg set ARC_USB_SMB_GRPName TestGroup
abscfg set ARC_USB_SAMBA_PrintDiskSpoolSpace 1024

#LPD
abscfg set ARC_USB_LPD_Enable 1
abscfg set ARC_USB_LPD_QUEUE_x_Name 0 lp0 
abscfg set ARC_USB_LPD_QUEUE_x_Name 1 lp1

